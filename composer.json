{
    "name": "ismailua/ubltr-invoice",
    "description": "A Laravel package for generating UBL-TR compliant XML invoices",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Ubl",
        "Ubl-TR",
        "Ubl-TR Invoice",
        "Ubl-TR Invoice Generator",
        "Ubl-TR Invoice Generator Laravel",
        "Ubl-TR Invoice Generator Laravel Package",
        "Ubl-TR Invoice Generator For Laravel"
    ],
    "authors": [
        {
            "name": "İsmail ÇAKIR",
            "email": "ismailua@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0",
        "ramsey/uuid": "^4.0",
        "ismailua/ubltr-invoice": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ismailua\\UblTrInvoice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ismailua\\UblTrInvoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ismailua\\UblTrInvoice\\UBLInvoiceServiceProvider"
            ],
            "aliases": {
                "UBLInvoice": "Ismailua\\UblTrInvoice\\Facades\\UBLInvoice"
            }
        }
    }
}
